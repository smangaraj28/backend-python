---Entity details
CREATE TABLE unihot.enity_branch (
    entityid 		           	varchar(100) NOT NULL REFERENCES unihot.enity(entityid),  -->hotelid
    entitybranchid              varchar(100)  NOT NULL,   -->branch id
    entityname                  varchar(100) NOT NULL,   -->branch name  -- Headoffice, koramangala
    entityemail                	varchar(100) NOT NULL,   -->branch EMAIL
    entityphone1                numeric(15),
    entityphone2                numeric(15),
    address                     varchar(500) NOT NULL,
    entitybranchstatus			varchar(1) NOT NULL,
    octime			           	timestamptz NOT NULL,
    lmtime			           	timestamptz NOT NULL,
    CONSTRAINT ucbr PRIMARY KEY (entityid, entitybranchid)  
    );
