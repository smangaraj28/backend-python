    CREATE TABLE webapp.trandetails(
    tran_id                       varchar(19) NOT NULL DEFAULT TO_CHAR(CURRENT_TIMESTAMP,'DDMMYYYY')||cast((nextval('webapp."tran_id_seq"')) AS TEXT) CONSTRAINT tran_unq_ref PRIMARY KEY,
    tran_orderdate                date NOT NULL,   ---> This is the date that impact entry in dailyposition date (backvalue dt????)
    tran_uniquereferencenumber    varchar(19) NOT NULL REFERENCES webapp.mforderdetails(mfor_uniquereferencenumber),
    tran_orderid                  varchar(8)  NOT NULL,
    tran_producttype              varchar(5)  NOT NULL, --> BSEMF-mutual fund, EQ-stock, IN-insurance, SGB-goldbondtran
    tran_ordertype                varchar(10) NOT NULL, --> ONETIME,SIPTRAN,OVERNIGHT,SWITCH
    tran_ordersubtyp              varchar(10),           --> --,[SIPTRAN - XSIP,ISIP,ESIP],--,--,
    tran_pfuserid 			      varchar(40) NOT NULL,
    tran_pfportfolioid            varchar(50) REFERENCES webapp.pfmaindetail(pfportfolioid),
    tran_amt                      numeric(19,2) NOT NULL,
    tran_nav                      numeric(19,4) NOT NULL,
    tran_unit                     numeric(19,2) NOT NULL,
    tran_invamount                numeric(19,2) NOT NULL,
    tran_buysell                  varchar(2) NOT NULL,    -->  P/R/SO/SI (Pruchar/Redemption/switchout/switchin)
    tran_remarks                  text,
    tran_sipregnno                varchar(20), 
    tran_beneficiaryid            varchar(16), 
    tran_dptxn                    varchar(10) NOT NULL,    --> C/N/P  (CDSL/NSDL/PHYSICAL)
    tran_foliono                  text NOT NULL,
    tran_userid                   integer NOT NULL,
    tran_memberid                 varchar(20) NOT NULL,
    tran_clientcode               varchar(20) NOT NULL,
    tran_schemecd                 varchar(20) NOT NULL,
    tran_schmname                 text NOT NULL,
    tran_settno                   varchar(14),
    tran_setttype                 varchar(2),
    tran_dailypositionflg         varchar(1) NOT NULL,  --> Y-added to webapp.dailyposition, N- Not yet added
    tran_fifobalunit              numeric(19,2) NOT NULL,   -->sum of this field always match dpos_unit
    tran_octime	                  timestamptz NOT NULL,
    tran_lmtime                   timestamptz NOT NULL,
	tran_entityid                 varchar(10) NOT NULL
	)

	
	CREATE TABLE webapp.dailyposition(
	dpos_date                     date NOT NULL,
	dpos_pfuserid 			      varchar(40) NOT NULL,
    dpos_pfportfolioid            varchar(50) REFERENCES webapp.pfmaindetail(pfportfolioid),
    dpos_producttype              varchar(5)  NOT NULL, --> BSEMF-mutual fund, EQ-stock, IN-insurance, SGBtran
	dpos_schemecd                 varchar(20) NOT NULL,	
	dpos_schmname                 text NOT NULL,
	dpos_invamount                numeric(19,2) NOT NULL,
    dpos_unit                     numeric(19,2) NOT NULL,
    dpos_unit_blocked             numeric(19,2) DEFAULT 0, --> This for orders not submitted to Exchange (relase this on validation failure)
    dpos_unit_utilised            numeric(19,2) DEFAULT 0, --> This for orders submitted to exchange (Release this if allotment failed)
	dpos_avgnav                   numeric(19,4) NOT NULL,
	dpos_curnav                   numeric(19,4) NOT NULL,
	dpos_curvalue                 numeric(19,2) NOT NULL,
	dpos_totalpnl                 numeric(19,2) NOT NULL,
    dpos_octime	                  timestamptz NOT NULL,
    dpos_lmtime                   timestamptz NOT NULL,
	dpos_entityid                 varchar(10) NOT NULL,
	CONSTRAINT unq_comb_dpos UNIQUE (dpos_entityid,dpos_pfuserid,dpos_pfportfolioid,dpos_producttype,dpos_schemecd,dpos_date) 
	)
	
	CREATE TABLE webapp.dailyposition_hist(
	dpos_date                     date NOT NULL,
	dpos_pfuserid 			      varchar(40) NOT NULL,
    dpos_pfportfolioid            varchar(50) REFERENCES webapp.pfmaindetail(pfportfolioid),
    dpos_producttype              varchar(5)  NOT NULL, --> BSEMF-mutual fund, EQ-stock, IN-insurance, SGBtran
	dpos_schemecd                 varchar(20) NOT NULL,	
	dpos_schmname                 text NOT NULL,
	dpos_invamount                numeric(19,2) NOT NULL,
    dpos_unit                     numeric(19,2) NOT NULL,
    dpos_unit_blocked             numeric(19,2) DEFAULT 0, --> This for orders not submitted to Exchange (relase this on validation failure)
    dpos_unit_utilised            numeric(19,2) DEFAULT 0, --> This for orders submitted to exchange (Release this if allotment failed)
	dpos_avgnav                   numeric(19,4) NOT NULL,
	dpos_curnav                   numeric(19,4) NOT NULL,
	dpos_curvalue                 numeric(19,2) NOT NULL,
	dpos_totalpnl                 numeric(19,2) NOT NULL,
    dpos_octime	                  timestamptz NOT NULL,
    dpos_lmtime                   timestamptz NOT NULL,
	dpos_entityid                 varchar(10) NOT NULL
	)
	
	    ---Table that stores the folio id for the funds under AMC IN a portfolio          
    CREATE TABLE webapp.mffoliodetails(
    fopfportfolioid        varchar(50) REFERENCES webapp.pfmaindetail(pfportfolioid),	
    foamcnatcode           varchar(10) REFERENCES webapp.amcmaster(amcnatcode),
    fopfamcfolionumber     varchar(50) NOT NULL,
    foorderoctime		   timestamptz NOT NULL,
    foorderlmtime      	   timestamptz NOT NULL,
    foentityid             varchar(10) NOT NULL
    )
	
############## FUTURE WORK ############
    CREATE TABLE webapp.batchjobsetup(
        bjse_jobgroup             varchar(50) NOT NULL,
        bjse_grpsequence          numeric,
        bjse_jobid                varchar(50) NOT NULL,
        bjse_jobsequence          numeric,
        bjse_freq                 varchar(2) NOT NULL, --D-Daily, W- Weekly, E - Every dayof week(0-Sun,1-Mon,2-Tue...),M-Monthly, Y - Yearly, F- Fortnightly, A - Adhoc, MI - Minue, HH - HOUR
                                                        --H - Run only on holiday, PH - Run only on Public holiday, 
        bjse_freq_count           numeric,   (eg: bjse_freq = MI and this field has 5 means it is every 5 mins)
        bjse_targetfunc           varchar(200),
        bjse_parameter            text,
        bjse_prevfunhand          varchar(2) NOT NULL, --(previously started job still running) S -skip starting new job now, N -Start New job, R- Run after current job complete
        bjse_errormsg             text,
        bjse_errorhand            varchar(2) NOT NULL, --S - Stop all, K - Skip and proceed, R -Retry
        bjse_retrynum             numeric,
        bjse_octime	              timestamptz NOT NULL,
        bjse_lmtime               timestamptz NOT NULL,
        bjse_entityid             varchar(10) NOT NULL
	)
############## FUTURE WORK ############
	
	In Future build this table based on batchjobsetup table
	
	
	
	  ------ create a sequence (this is to be reset at 00:00 hrs every day, Lambda function to be written for reset)
          CREATE SEQUENCE webapp.bjs_id_seq 
          START 1
          increment 1
          MAXVALUE 999999999;
          
          
	
	CREATE TABLE webapp.batchjobstatus(
        bjs_id                   varchar(19) NOT NULL DEFAULT TO_CHAR(CURRENT_TIMESTAMP,'DDMMYYYY')||cast((nextval('webapp."bjs_id_seq"')) AS TEXT) CONSTRAINT bjs_pkey PRIMARY KEY,
        bjs_businessdate         date NOT NULL,
        bjs_jobgroup             varchar(50) NOT NULL,
        bjs_jobid                varchar(50) NOT NULL,
        bjs_scheduletime         timestamptz,
        bjs_starttime            timestamptz,
        bjs_endtime              timestamptz,
        bjs_status               varchar(20) NOT NULL, N-Not Started, P- Pending to start, R-Running,C- Completed,F- has Failure, S - Skipped as per setup, A- Runafter current job complete
        bjs_errormsg             text,
        bjs_octime	             timestamptz NOT NULL,
        bjs_lmtime               timestamptz NOT NULL,
        bjs_entityid             varchar(10) NOT NULL
	)
	
    CREATE TABLE webapp.batchjobstatus_hist(
        bjs_businessdate         date NOT NULL,
        bjs_jobgroup             varchar(50) NOT NULL,
        bjs_jobid                varchar(50) NOT NULL,        
        bjs_starttime            timestamptz NOT NULL,
        bjs_endtime              timestamptz,
        bjs_status               varchar(20) NOT NULL, R-Running,C- Completed,F- has Failure
        bjs_errormsg             text,
        bjs_octime	             timestamptz NOT NULL,
        bjs_lmtime               timestamptz NOT NULL,
        bjs_entityid             varchar(10) NOT NULL
	)
	
	
	
    




