VALUES ('lIYuGM2274aDIWCgIf6COXK81NC2','admin','A','A','A','N',CURRENT_TIMESTAMP,CURRENT_TIMESTAMP,CURRENT_TIMESTAMP,'NAWALCUBE','IN');


---user access permission table
CREATE TABLE ncusr.useraccess (
    userid 		               varchar(100) NOT NULL REFERENCES ncusr.userlogin(userid),
    site                       varchar(100),  --> nc - Nawalcube, dv - developer, au - auth
    role                       varchar(100),  --> ADMIN,READONLY,WRITE,NODELETE
    accessstatus	           varchar(2) NOT NULL, --> (A- Active, B-Blocked) for the site
    octime			           timestamptz NOT NULL,
    lmtime			           timestamptz NOT NULL,
    entityid                   varchar(20) NOT NULL,
    countryid                  varchar(10) NOT NULL,
    CONSTRAINT us PRIMARY KEY (userid, site)  
    );


