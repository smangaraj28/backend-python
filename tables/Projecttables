-- Login history
CREATE TABLE ncusr.loginh (
    userid 		               varchar(100) NOT NULL REFERENCES ncusr.userlogin(userid),
    ipaddress                  varchar(25),
    sessionid                  varchar(100),
    site                       varchar(100),  --> nc - Nawalcube, dv - developer, au - auth
    logintime                  timestamptz NOT NULL, 
    logoutime                  timestamptz ,
    entityid                   varchar(20) NOT NULL,
    countryid                  varchar(10) NOT NULL
);

-- App details
        CREATE SEQUENCE ncapp.app_pk_seq 
            START 1
            increment 1
            MAXVALUE 2147483600;  -- Maximum of integer
        

CREATE TABLE ncapp.appdetail (
    apppk                      integer NOT NULL DEFAULT nextval('ncapp."app_pk_seq"') CONSTRAINT apppkey PRIMARY KEY,
    appname                    text, 
    appusertype                text, --(I - Investor, D - MFD, A - RIA, P - Portfolio tool, T - Trusted app )
    redirecturi                text,
    postbackuri                text,
    description                text,
    starmfdet                  text,
    appid                      text,
    appkey                     text,
    expirydate                 date NOT NULL,
    approved                   varchar(2) NOT NULL, --> Y - Approved, N - Not approved only for Trusted app
    product                    varchar(30) NOT NULL,
    delflg                     varchar(1), --> Y - Deleted, N - Not Deleted
    appuserid 		           varchar(100) NOT NULL REFERENCES ncusr.userlogin(userid),
    octime			           timestamptz NOT NULL,
    lmtime			           timestamptz NOT NULL,
    entityid                   varchar(20) NOT NULL,
    countryid                  varchar(10) NOT NULL
);


---Secret key detail table

CREATE TABLE ncapp.secrettkn (
	secretcode 		            varchar(100) NOT NULL CONSTRAINT secretcode PRIMARY KEY, 
    seccdid 		            varchar(30) NOT NULL,  --> DDMMYYYYHHMMSS
    secoctime			        timestamp NOT NULL,
    entityid                    varchar(10) NOT NULL,
    countryid                   varchar(10) NOT NULL
    );
    

-- User authentication token
CREATE TABLE ncusr.userauth (
    userid 		               varchar(100) NOT NULL REFERENCES ncusr.userlogin(userid),
    appid                      text NOT NULL,
    userauthtkn                varchar(400) NOT NULL,
    tknexpiry                  timestamptz NOT NULL,
    entityid                   varchar(20) NOT NULL,
    countryid                  varchar(10) NOT NULL,
    octime			           timestamptz NOT NULL,
    lmtime			           timestamptz NOT NULL,
    CONSTRAINT unq_comb_uauth UNIQUE (userid,appid) 
);


-- APP authentication token
CREATE TABLE ncapp.appusrauth (
    userauthtkn                varchar(400),
    appid                      text NOT NULL,
    passwordtkn                text,
    passwordtknexpiry          timestamptz NOT NULL,
    entityid                   varchar(20) NOT NULL,
    countryid                  varchar(10) NOT NULL,
    octime			           timestamptz NOT NULL,
    lmtime			           timestamptz NOT NULL,
    CONSTRAINT unq_comb_auauth UNIQUE (userauthtkn,appid) 
);










    
